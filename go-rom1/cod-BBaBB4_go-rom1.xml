    <!-- Romans 1 -->
    <body>
      <div xml:id="go-rom1">
        <head>Ad Romanos 1</head>
        <div>
          <head>Versa 1</head>
          <p>
            <quote ana="#rom1_1" source="http://scta.info/resource/bible-rom1_1@1-1">paulus</quote>
            hebraice quietus graece modicus latine
          </p>
          <p>
            <quote ana="#rom1_1">servus</quote>
            nomen humilitatis ut ad eam provocet
          </p>
          <p>
            <quote ana="#rom1_1">ihesu</quote>
            salvatoris cui merito omnes servi cui ser<lb ed="#S"/>vire regnare 
          </p>
          <p>
            <quote ana="#rom1_1">christi</quote>
            regis et sacerdotis
          </p>
          <p>
            <quote ana="#rom1_1">vocatus</quote>
            aeterna electione et temporali missione
          </p>
          <p>
            <quote ana="#rom1_1">apostolus</quote>
            missus legatus vel nuncius
          </p>
          <p>
            <quote ana="#rom1_1">segregatus in</quote>
            ab utero sinagoge in qua quasi mortuus fuit
          </p>
          <p>
            <quote ana="#rom1_1">evangelium dei</quote>
            praedicandum ecce officium 
          </p>
        </div>
        <div>
          <head>Versa 2</head>
          <p>
            <quote ana="#rom1_2">quod</quote>
            commendatio evangelii a quattuor
          </p>
          <p>
            <quote ana="#rom1_2">ante</quote>
            completionem
          </p>
          <p>
            <quote ana="#rom1_2">promiserat per</quote>
            deus pater 
            gratis 
            ut rem utilem et necessariam
          </p>
          <p>
            <quote ana="#rom1_2">prophetas suos in</quote>
            qui ex deo non ex se magni
            a tempore 
            ab autore 
            a testibus <unclear>co.</unclear>
          </p>
          <p>
            <quote ana="#rom1_2">scripturis sanctis</quote>
            non modo verbis 
            ne daretur oblivioni 
          </p>
        </div>
        <div>
          <head>Versa 3</head>
          <p>
            <quote ana="#rom1_3">de</quote>
            factis
          </p>
          <p>
            <quote ana="#rom1_3">filio</quote>
            quia omnes ad filium ducunt
          </p>
          <p>
            <quote ana="#rom1_3">suo qui factus est</quote>
            proprio commendatio evangelii a materia 
          </p>
          <p>
            <quote ana="#rom1_3">ei</quote>
            ad honorem eius
          </p>
          <p>
            <quote ana="#rom1_3">ex semine david</quote>
            commendatio autoris a quattuor primo ab origine
          </p>
          <p>
            <quote ana="#rom1_3">secundum</quote>
            factus ne putetur homo tantum et non secundum divinitatem
          </p>
          <p>
            <quote ana="#rom1_3">carnem</quote>
            quia <quote ana="#io1_14">verbum caro factum</quote> etc
          </p>
        </div>
      </div>
    </body>
